{
  "name": "expo-tiktok-sdk",
  "version": "0.1.0",
  "description": "A module for integrating TikTok SDK with Expo projects to enable TikTok-related functionalities",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "typecheck": "tsc --noEmit",
    "build": "expo-module build",
    "build:plugin": "tsc --build plugin/tsconfig.json",
    "prepare": "npm run build && npm run build:plugin"
  },
  "keywords": [
    "react-native",
    "expo",
    "expo-tiktok-sdk",
    "ExpoTiktokSdk",
    "tiktok",
    "ads"
  ],
  "repository": "https://github.com/abdlrhmnnn/expo-tiktok-sdk.git",
  "bugs": {
    "url": "https://github.com/abdlrhmnnn/expo-tiktok-sdk.git/issues"
  },
  "author": "Abdelrahman Mohammed <abdlrhmnnn@gmail.com> (https://github.com/abdlrhmnnn)",
  "license": "MIT",
  "homepage": "https://github.com/abdlrhmnnn/expo-tiktok-sdk.git#readme",
  "dependencies": {
    "expo-modules-core": "^1.11.0"
  },
  "devDependencies": {
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.0",
    "expo-module-scripts": "^4.0.4",
    "expo": "~52.0.0",
    "react-native": "0.72.10",
    "react-native-typescript-transformer": "^1.2.13",
    "@expo/config-plugins": "^7.0.0",
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0",
    "@babel/core": "^7.20.0"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "expo": {
    "plugin": "./app.plugin.js"
  },
  "files": [
    "build",
    "plugin/build",
    "app.plugin.js",
    "src",
    "android",
    "ios"
  ]
}
